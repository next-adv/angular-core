{
  "name": "@next-adv/angular-core",
  "version": "5.3.2",
  "author": "Riccardo Coco, Giuseppe Matrella",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "for i in schematics/*/schema.json; do mkdir -p `dirname ../../dist/angular-core/$i`; cp -R $i ../../dist/angular-core/$i; done",
    "copy:files": "for i in schematics/*/files/**; do mkdir -p `dirname ../../dist/angular-core/$i`; cp -R $i ../../dist/angular-core/$i; done",
    "copy:collection": "cp schematics/collection.json ../../dist/angular-core/schematics/collection.json",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection"
  },
  "private": false,
  "dependencies": {
    "schematics-utilities": "^1.1.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.23",
    "@angular-devkit/build-ng-packagr": "~0.801.3",
    "@angular/animations": "~8.1.2",
    "@angular/cli": "~8.1.2",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/language-service": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "^8.2.14",
    "@ionic/storage": "^2.2.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^5.1.0",
    "protractor": "~5.4.0",
    "rxjs": "^6.5.4",
    "ts-node": "~7.0.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "zone.js": "~0.9.1"
  },
  "peerDependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/cli": "~8.1.2",
    "@angular/common": "^8.2.14",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/language-service": "~8.1.2",
    "@angular/router": "^8.2.14",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "~4.0.0",
    "zone.js": "~0.9.1",
    "@ionic/storage": "^2.2.0",
    "rxjs": "^6.5.4"
  },
  "schematics": "./schematics/collection.json"
}
